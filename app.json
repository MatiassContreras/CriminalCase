      "package": "com.matiasscontreras.CriminalCaseExpo",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f45e834e-3a7c-4fbb-afb1-03674919ce4a"
